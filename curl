curl -v -H "Accept: application/json" -X POST http://localhost:4000/subscribe/twitter/tag/yolo
curl -v -H "Accept: application/json" -X POST http://localhost:4000/subscribe/twitter/tag/iphone

curl -v -H "Accept: application/json" -X POST http://localhost:3000/board/my-board/twitter/yolo
curl -v -H "Accept: application/json" -X POST http://localhost:3000/board/my-board/twitter/iphone

